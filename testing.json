{
  "type": "e2e",
  "findReplace": {
    "launch voice app": "open guess the price"
  },
  "homophones": {
    "is": ["as", "does", "it's", "has"],
    "two": ["to", "2"],
    "contestant": ["contested"]
  },
  "trace": false,
  "jest": {
    "reporters": [
        "default",
        [
          "./node_modules/jest-html-reporter",
          {
             "includeFailureMsg": true,
        	   "pageTitle": "Bespoken Test Report"
          }
        ]
      ],
    "silent": false,
    "testMatch": ["**/test/*.yml", "**/tests/*.yml", "**/*.e2e.yml", "**/*.spec.yml"]
},
  "virtualDeviceToken": {
    "alexa": {
      "en-US": "<VIRTUAL_DEVICE_TOKEN_FROM_BESPOKEN_DASHBOARD>"
      },
      "google": {
        "en-US": "<VIRTUAL_DEVICE_TOKEN_FROM_BESPOKEN_DASHBOARD>"
      }
    }
}