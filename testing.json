{
  "type": "e2e",
  "findReplace": {
    "INVOCATION_NAME": "open bring"
  },
  "homophones": {
    "bring": ["ring", "bing"],
    "lettuce": ["let us"],
    "figs": ["six","6","vicks"],
    "carrots": ["carried"]
  },
  "trace": false,
  "jest": {
    "reporters": [
        "default",
        [
          "./node_modules/jest-html-reporter",
          {
             "includeFailureMsg": true,
        	   "pageTitle": "Bespoken Test Report"
          }
        ]
      ],
    "silent": false,
    "testMatch": ["**/test/*.yml", "**/tests/*.yml", "**/*.e2e.yml", "**/*.spec.yml"]
},
  "virtualDeviceToken": {
    "alexa": {
      "en-US": "<VIRTUAL_DEVICE_TOKEN_FROM_BESPOKEN_DASHBOARD>",
      "de-DE": "<VIRTUAL_DEVICE_TOKEN_FROM_BESPOKEN_DASHBOARD>"
      },
      "google": {
        "en-US": "<VIRTUAL_DEVICE_TOKEN_FROM_BESPOKEN_DASHBOARD>"
      }
    }
}